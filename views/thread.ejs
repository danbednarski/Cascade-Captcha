  <script>
    // Example matrix
    const matrix = [
      [0, 0, 'b', 0, 'a'],
      [0, '4', 0, 'g', 0],
      [0, 0, 0, 0, 0],
      ['d', 0, 'z', 0, 0],
      ['d', 0, 'z', 0, 0],
      ['d', 0, 'z', 0, 0],
      ['d', 0, 'z', 0, 0]
    ];

    const matrixContainer = document.querySelector('.matrix');

    function renderMatrix(matrix) {
      matrixContainer.style.gridTemplateColumns = `repeat(${matrix[0].length}, auto)`; // Adjust grid columns dynamically
      
      matrix.forEach(row => {
        row.forEach(cell => {
          const cellElement = document.createElement('div');
          cellElement.classList.add('cell');
          if (cell === 0) {
            cellElement.classList.add('empty'); // Style empty cells differently
            cellElement.textContent = '.';
          } else {
            cellElement.textContent = cell;
          }
          matrixContainer.appendChild(cellElement);
        });
      });
    }

    renderMatrix(matrix);
  </script>
